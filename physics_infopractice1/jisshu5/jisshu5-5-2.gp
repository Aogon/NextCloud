#-------------------------------------------------------------------------------
# gnuplotの設定
#-------------------------------------------------------------------------------
reset
set nokey                 # 凡例の非表示
set xrange [0:100]       # x軸方向の範囲の設定
set yrange [0:100]       # y軸方向の範囲の設定
set size square           # 図を正方形にする

set term gif animate      # 出力をgifアニメに設定
set output "sample2.gif"  # 出力ファイル名の設定

#-------------------------------------------------------------------------------
# 変数の設定
#-------------------------------------------------------------------------------
n0 = 0    # ループ変数の初期値
n1 = 1000   # ループ変数の最大値
dn = 1    # ループ変数の増加間隔

#-------------------------------------------------------------------------------
# ループの開始
#-------------------------------------------------------------------------------
load "jisshu5-5-2.plt" 